{
  "name": "databento-mcp-server",
  "version": "1.0.0",
  "description": "Model Context Protocol (MCP) server for DataBento market data",
  "main": "dist/index.js",
  "bin": {
    "databento-mcp-server": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "test:once": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch"
  },
  "keywords": [
    "mcp",
    "databento",
    "market-data",
    "futures",
    "trading"
  ],
  "author": "",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.19.1",
    "dotenv": "^17.2.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/node": "^24.6.2",
    "@vitest/coverage-v8": "^1.6.1",
    "nock": "^13.5.6",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^1.6.1"
  }
}
